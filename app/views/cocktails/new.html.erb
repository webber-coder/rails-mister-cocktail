<div class="container" style="margin-top:100px">
<h1 style="color:white; text-align:center;">SHARE YOUR FAVOURITE COCKTAIL</h1>
<div class="form">
<%=simple_form_for @cocktail do |f|%>
<%=f.input :name%>
<%=f.input :description%>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%=f.submit "Add the cocktail"%>
<%end%>
</div>
</div>
